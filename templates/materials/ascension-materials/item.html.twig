<a href="#" class="ascmat">
    {% for icon in ascmat.icons|filter(icon => icon.role == 'Ascension mat: 4-star icon') %} 
        <div class="item-asc-mat four-star">
            <img src="/uploads/{{ icon.filename }}" alt="">
            <p>{{ ascmat.fourStarName }}</p>
        </div>
    {% endfor %}
    {% for icon in ascmat.icons|filter(icon => icon.role == 'Ascension mat: 3-star icon') %} 
        <div class="item-asc-mat three-star">
            <img src="/uploads/{{ icon.filename }}" alt="">
            <p>{{ ascmat.threeStarName }}</p>
        </div>
    {% endfor %}
    {% for icon in ascmat.icons|filter(icon => icon.role == 'Ascension mat: 2-star icon') %} 
        <div class="item-asc-mat two-star">
            <img src="/uploads/{{ icon.filename }}" alt="">
            <p>{{ ascmat.twoStarName }}</p>
        </div>
    {% endfor %}
</a>